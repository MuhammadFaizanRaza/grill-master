{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "rules": {
    "global-require": 0,
    "max-len": 0,
    "no-useless-catch": 0,
    "no-underscore-dangle": "off",
    "no-unused-vars": "off",
    "consistent-return": "off"
  }
}
